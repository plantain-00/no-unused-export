{
  "name": "no-unused-export",
  "version": "1.12.2",
  "description": "A CLI tool to check whether exported things in a module is used by other modules for Typescript.",
  "main": "index.js",
  "scripts": {
    "build": "clean-scripts build",
    "lint": "clean-scripts lint",
    "test": "clean-scripts test",
    "fix": "clean-scripts fix"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/plantain-00/no-unused-export.git"
  },
  "author": "York Yao",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/plantain-00/no-unused-export/issues"
  },
  "typeCoverage": {
    "atLeast": 99.17
  },
  "homepage": "https://github.com/plantain-00/no-unused-export#readme",
  "devDependencies": {
    "@types/glob": "7.1.3",
    "@types/minimist": "1.2.0",
    "@types/node": "14.0.27",
    "@types/parse5": "5.0.3",
    "@typescript-eslint/eslint-plugin": "3.8.0",
    "@typescript-eslint/parser": "3.8.0",
    "clean-release": "2.13.0",
    "clean-scripts": "1.17.0",
    "eslint": "7.6.0",
    "eslint-config-prettier": "6.11.0",
    "eslint-plugin-plantain": "1.0.0",
    "markdownlint-cli": "0.23.2",
    "rimraf": "3.0.2",
    "ts-node": "8.10.2",
    "tsconfig-plantain": "0.0.1",
    "type-coverage": "2.9.0"
  },
  "dependencies": {
    "glob": "7",
    "minimist": "1",
    "parse5": "6",
    "postcss-less": "3",
    "postcss-scss": "2",
    "ts-lib-utils": "2",
    "tslib": "1 || 2",
    "typescript": "3 || 4"
  },
  "bin": {
    "no-unused-export": "bin/no-unused-export"
  }
}

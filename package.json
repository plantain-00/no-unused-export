{
  "name": "no-unused-export",
  "version": "1.15.0",
  "description": "A CLI tool to check whether exported things in a module is used by other modules for Typescript.",
  "main": "index.js",
  "scripts": {
    "build": "clean-scripts build",
    "lint": "clean-scripts lint",
    "test": "clean-scripts test",
    "fix": "clean-scripts fix",
    "release": "clean-release"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/plantain-00/no-unused-export.git"
  },
  "author": "York Yao",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/plantain-00/no-unused-export/issues"
  },
  "typeCoverage": {
    "atLeast": 99.22
  },
  "homepage": "https://github.com/plantain-00/no-unused-export#readme",
  "devDependencies": {
    "@types/glob": "8.0.0",
    "@types/minimist": "1.2.2",
    "@types/node": "18.11.10",
    "@types/parse5": "6.0.3",
    "@typescript-eslint/eslint-plugin": "5.45.0",
    "@typescript-eslint/parser": "5.45.0",
    "clean-release": "2.17.1",
    "clean-scripts": "1.20.2",
    "eslint": "8.29.0",
    "eslint-config-prettier": "8.5.0",
    "eslint-plugin-plantain": "2.0.0",
    "markdownlint-cli": "0.32.2",
    "rimraf": "3.0.2",
    "ts-node": "10.9.1",
    "tsconfig-plantain": "0.0.2",
    "type-coverage": "2.24.1"
  },
  "dependencies": {
    "glob": "7 || 8",
    "minimist": "1",
    "parse5": "6",
    "postcss": "8",
    "postcss-less": "3 || 4 || 5 || 6",
    "postcss-scss": "2 || 3 || 4",
    "tslib": "1 || 2",
    "typescript": "3 || 4"
  },
  "bin": {
    "no-unused-export": "bin/no-unused-export"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org/"
  }
}

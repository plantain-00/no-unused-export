{
  "name": "no-unused-export",
  "version": "1.14.0",
  "description": "A CLI tool to check whether exported things in a module is used by other modules for Typescript.",
  "main": "index.js",
  "scripts": {
    "build": "clean-scripts build",
    "lint": "clean-scripts lint",
    "test": "clean-scripts test",
    "fix": "clean-scripts fix",
    "release": "clean-release"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/plantain-00/no-unused-export.git"
  },
  "author": "York Yao",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/plantain-00/no-unused-export/issues"
  },
  "typeCoverage": {
    "atLeast": 99.17
  },
  "homepage": "https://github.com/plantain-00/no-unused-export#readme",
  "devDependencies": {
    "@types/glob": "7.1.3",
    "@types/minimist": "1.2.1",
    "@types/node": "14.14.14",
    "@types/parse5": "5.0.3",
    "@typescript-eslint/eslint-plugin": "4.10.0",
    "@typescript-eslint/parser": "4.10.0",
    "clean-release": "2.15.2",
    "clean-scripts": "1.18.7",
    "eslint": "7.16.0",
    "eslint-config-prettier": "7.0.0",
    "eslint-plugin-plantain": "1.1.0",
    "markdownlint-cli": "0.26.0",
    "rimraf": "3.0.2",
    "ts-node": "9.1.1",
    "tsconfig-plantain": "0.0.1",
    "type-coverage": "2.14.6"
  },
  "dependencies": {
    "glob": "7",
    "minimist": "1",
    "parse5": "6",
    "postcss-less": "3 || 4",
    "postcss-scss": "2 || 3",
    "ts-lib-utils": "2",
    "tslib": "1 || 2",
    "typescript": "3 || 4"
  },
  "bin": {
    "no-unused-export": "bin/no-unused-export"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org/"
  }
}
